<h1>Make Changes to the Itinerary</h1>

<table>
  <thead>
    <tr>
      <th>Time</th>
	  <th>Description</th>
    </tr>
  </thead>
  
  <tbody>
	<% @itinerary.each do |event| %>
		<tr>
			<td><%= event.start_time %> through <%= event.end_time %></td>
			<td><%= event.desc %></td>
		</tr>
	<% end %>
  </tbody>
</table>




