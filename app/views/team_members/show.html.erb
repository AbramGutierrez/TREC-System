<h1>Show team members</h1>

<% @participants.each do |participant| %>
	<% @id = participant.id %>
	<p>
		<% @account = Account.find(@id) %>
		<%= @account.first_name %>
		<%= @account.last_name %>
		| <%= link_to "Remove" %>
	</p>
<% end %>


<li><%= link_to "Add new team member", controller: "participants", action: "new" %></li>