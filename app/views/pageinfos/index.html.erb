<h1>Listing Pageinfos</h1>

<table>
  <thead>
    <tr>
      <th>Page</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pageinfos.each do |pageinfo| %>
      <tr>
        <td><%= pageinfo.page %></td>
        <td><%= pageinfo.body %></td>
        <td><%= link_to 'Show', pageinfo %></td>
        <td><%= link_to 'Edit', edit_pageinfo_path(pageinfo) %></td>
        <td><%= link_to 'Destroy', pageinfo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pageinfo', new_pageinfo_path %>
